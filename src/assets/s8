P.when("A", "jQuery").execute(function(A, $) {
$("#selectQuantity [name='quantity'], #mobileQuantityDropDown").live("change", function (event) {
if (event.updateDDMOnQuantityChange) {
return;
}
event.updateDDMOnQuantityChange = 1;
var quantity = $(this).val();
var asin = "B01GVRK9ZG";
var deviceType = "web";
if (!asin || !quantity) {
return;
}
var canReload = false;
if (typeof sbrContext !== 'undefined') {
canReload = sbrContext.sbrBehavior && sbrContext.sbrBehavior.reloadPageWhenBBWinnerChange;
}
var merchantId = "";
var preselectedMerchantId = "";
if(canReload) {
preselectedMerchantId = "";
} else {
merchantId = "";
}
$.ajax({
type: "POST",
url: "/gp/product/features/dynamic-delivery-message/udp-ajax-handler/get-delivery-message.html?_encoding=UTF-8",
contentType: 'application/x-www-form-urlencoded;charset=utf-8',
data: {
"asin": asin,
"deviceType": deviceType,
"merchantId": merchantId,
"preselectedMerchantId": preselectedMerchantId,
"quantity": quantity
},
dataType: "html",
success: function(objResponse) {
if (objResponse != null && objResponse != "") {
$("#dynamicDeliveryMessage").replaceWith(objResponse);
}
}
});
});
});
